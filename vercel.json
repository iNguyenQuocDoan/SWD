{
  "version": 2,
  "builds": [
    {
      "src": "BE/api/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "FE/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "BE/api/index.ts"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "BE/api/index.ts": {
      "maxDuration": 30
    }
  }
}
